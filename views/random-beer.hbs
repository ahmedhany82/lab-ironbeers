{{#each random}}
<div class="random"> 
    <div class="randomPartial">
        {{> beerpartial this}}
    </div>
    <div class="random-addon">
        <div class="pairing">
            <h5>Food Pairing</h5>
            <ul>
                {{#each this.food_pairing}}
                    <li>{{this}}</li>
                {{/each}}
            </ul>
        </div>
        <div class="brewersTips">
            <h5>Brewers Tips</h5>
            <p>{{this.brewers_tips}}</p>
        </div>
    </div>
</div>

{{/each}}